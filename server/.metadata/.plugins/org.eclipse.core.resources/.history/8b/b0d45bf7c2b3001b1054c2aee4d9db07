package com.dev04.server.DAO;

import org.junit.jupiter.api.Test;
import org.junit.jupiter.api.extension.ExtendWith;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.test.context.junit.jupiter.SpringExtension;

@ExtendWith(SpringExtension.class)
class MemberServiceTest {
	
	@Autowired
	MemberService ms;

	@Test
	public void getMemberInfo() {
		Member member = new Member();
		member.setId("hou");
		member.setPassword("hoho");
		member.setName("호우");
		member.setEmail("hou@naver.com");
		member.setBirthday("2021-05-13");
		member.setPhoneNumber("01012345678");
		member.setAddress1("a");
		member.setAddress2("b");
		member.setZipcode("12345");
		member.setRegDate("2021-05-13");
		ms.signUpMember(member);
	}

}
